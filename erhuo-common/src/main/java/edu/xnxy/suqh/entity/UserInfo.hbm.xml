<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="edu.xnxy.suqh.entity.UserInfo" table="user_info">
        <id name="userId" type="java.lang.Integer">
            <column name="user_id"/>
            <generator class="identity" />
        </id>
        <property name="userName" type="java.lang.String">
            <column name="user_name" length="20" not-null="true" />
        </property>
        <property name="userPassword" type="java.lang.String">
            <column name="user_password" length="20" not-null="true" />
        </property>
        <property name="userGender" type="java.lang.String">
            <column name="user_gender" length="5" not-null="true" />
        </property>
        <property name="userAge" type="java.lang.Integer">
            <column name="user_age" length="2" not-null="true" />
        </property>
        <property name="userEmail" type="java.lang.String">
            <column name="user_email" length="20" not-null="false" />
        </property>
        <property name="userAdderss" type="java.lang.String">
            <column name="user_address" length="30" not-null="true" />
        </property>
        <property name="userPhone" type="java.lang.Long">
            <column name="user_phone" length="15" not-null="true" />
        </property>
        <property name="userStatus" type="java.lang.Integer">
            <column name="user_status" length="1" not-null="false" />
        </property>
        <property name="userGradeid" type="java.lang.String">
            <column name="user_gradeid" length="20" not-null="false" />
        </property>
        <property name="userIsonline" type="java.lang.Integer">
            <column name="user_isonline" length="1" not-null="false" />
        </property>
        <property name="userPoint" type="java.lang.Integer">
            <column name="user_point" length="4" not-null="false" />
        </property>
        <property name="isManager" type="java.lang.Integer">
            <column name="isManager" length="1" not-null="false" />
        </property>
        <property name="outDate" type="java.lang.Long">
            <column name="outDate" length="20" not-null="false" />
        </property>
        <property name="validataCode" type="java.lang.String">
            <column name="validataCode" length="64" not-null="false" />
        </property>
        <property name="ext1" type="java.lang.Integer">
            <column name="ext1" length="7" not-null="false" />
        </property>
        <property name="ext2" type="java.lang.String">
            <column name="ext2" length="20" not-null="false" />
        </property>
        <property name="ext3" type="java.lang.String">
            <column name="ext3" length="20" not-null="false" />
        </property>
    </class>
</hibernate-mapping>